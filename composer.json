{
    "name": "yien/fat-cms",
    "description": "Fat CMS Based on Yii 2 Advanced Project Template",
    "keywords": ["fatcms", "fat", "cms", "yii2"],
    "homepage": "https://fatcms.xyz/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yien/fat-cms/issues/new",
        "source": "https://github.com/yien/fat-cms"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.6",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "yiisoft/yii2-redis": "~2.0.0",
        "yiisoft/yii2-imagine": "^2.1",
        "yiisoft/yii2-authclient": "^2.1",
        "yiisoft/yii2-httpclient": "^2.0",
        "yiisoft/yii2-queue": "^2.1",
        "vlucas/phpdotenv": "^2.5",
        "dmstr/yii2-adminlte-asset": "^2.1"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
